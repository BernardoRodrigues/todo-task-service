{
  "name": "task-service",
  "version": "1.0.0",
  "description": "Task microservice",
  "main": "index.js",
  "scripts": {
    "build:dev": "tsc && copyfiles -f src/cert/*.crt dist/src/cert && copyfiles -f src/cert/*.key dist/src/cert && copyfiles -f package.json dist && copyfiles -f src/files/*.json dist/src/files",
    "start:dev": "NODE_ENV=dev node dist/src/index.js",
    "build:prod": "tsc",
    "start:prod": "NODE_ENV=production node dist/src/index.js",
    "prepare": "npm run build:prod"
  },
  "author": "Bernardo Rodrigues",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.3",
    "@types/node-schedule": "^1.3.1",
    "@types/pg": "^7.14.11",
    "@types/web-push": "^3.3.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node": "^16.1.0",
    "node-schedule": "^2.0.0",
    "pg": "^8.6.0",
    "typescript": "^4.3.5",
    "web-push": "^3.4.4"
  },
  "devDependencies": {}
}
